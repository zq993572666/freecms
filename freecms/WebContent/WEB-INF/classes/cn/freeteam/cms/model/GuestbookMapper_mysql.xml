  <select useCache="false" id="guestbookUpdateWeek" parameterType="cn.freeteam.cms.model.GuestbookExample" resultMap="BaseResultMap">
	SELECT COUNT(id) countnum,
	(CASE 
	WHEN WEEKDAY(ADDTIME)=0 THEN '星期一' 
	WHEN WEEKDAY(ADDTIME)=1 THEN '星期二' 
	WHEN WEEKDAY(ADDTIME)=2 THEN '星期三' 
	WHEN WEEKDAY(ADDTIME)=3 THEN '星期四' 
	WHEN WEEKDAY(ADDTIME)=4 THEN '星期五' 
	WHEN WEEKDAY(ADDTIME)=5 THEN '星期六' 
	WHEN WEEKDAY(ADDTIME)=6 THEN '星期七' 
	END
	) AS datename FROM freecms_guestbook  
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
 	GROUP BY WEEKDAY(ADDTIME)
  </select>
  
  
  <select useCache="false" id="sysSiteGuestbookPage" parameterType="cn.freeteam.cms.model.GuestbookExample" resultMap="BaseResultMap">
   	SELECT COUNT(g.id) AS countnum ,siteid,max(s.name) sitename
	FROM freecms_guestbook  g
	LEFT JOIN freecms_site s ON siteid=s.id 
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
 	GROUP BY siteid ORDER BY countnum desc
    limit ${skip},${pageSize}
  </select>
